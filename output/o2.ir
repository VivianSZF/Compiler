FUNCTION add :
PARAM temp
t6 := #0 * #4
t3 := temp + t6
t1 := *t3
t10 := #1 * #4
t7 := temp + t10
t2 := *t7
t0 := t1 + t2
RETURN t0
FUNCTION main :
DEC op 8
DEC r 8
v4 := #0
v5 := #0
LABEL l0 :
IF v4 < #2 GOTO l1
GOTO l2
LABEL l1 :
LABEL l3 :
IF v5 < #2 GOTO l4
GOTO l5

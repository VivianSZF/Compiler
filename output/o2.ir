t20 := v5 * #4
t17 := &op + t20
t21 := v4 + v5
*t17 := t21
t25 := v5 + #1
v5 := t25
GOTO l3
LABEL l5 :
t34 := #0 * #8
t29 := &r + t34
t31 := v4 * #4
t28 := t29 + t31
ARG &op
t35 := CALL add
*t28 := t35
t44 := #0 * #8
t39 := &r + t44
t41 := v4 * #4
t38 := t39 + t41
t37 := *t38
WRITE t37
t46 := v4 + #1
v4 := t46
